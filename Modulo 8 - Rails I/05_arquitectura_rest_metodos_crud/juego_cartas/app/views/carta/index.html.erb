<p id="notice"><%= notice %></p>

<h1>Carta</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carta.each do |cartum| %>
      <tr>
        <td><%= cartum.nombre %></td>
        <td><%= cartum.descripcion %></td>
        <td><%= link_to 'Show', cartum %></td>
        <td><%= link_to 'Edit', edit_cartum_path(cartum) %></td>
        <td><%= link_to 'Destroy', cartum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cartum', new_cartum_path %>
